{
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      "always"
    ],
    "no-console": [
      true,
      "log",
      "warn"
    ],
    "trailing-comma": [
      true,
      "never"
    ],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "(^_|msx)"
      }
    ],
    "arrow-parens": true
  }
}